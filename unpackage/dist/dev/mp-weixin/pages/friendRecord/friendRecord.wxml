<view><view class="list"><block wx:for="{{recordlist}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list-item"><view class="msg"><view style="font-size:28rpx;text-align:left;">{{item.name}}</view><view style="font-size:26rpx;text-align:center;">{{item.tel}}</view><view style="font-size:24rpx;text-align:right;color:#888;letter-spacing:1rpx;">{{item.time}}</view></view><block wx:if="{{!item.status}}"><view class="btns2"><button data-event-opts="{{[['tap',[['handIn',[index]]]]]}}" class="cu-btn round line-red" bindtap="__e">提交门店审核</button><button class="cu-btn round line-red" style="margin-left:30rpx;">取消</button></view></block><block wx:else><view class="wait">待门店审核</view></block></view></block></view><view class="{{['cu-modal',showModel?'show':'']}}"><view class="cu-dialog phone-box"><text data-event-opts="{{[['tap',[['closeModel',['$event']]]]]}}" class="lg text-gray cuIcon-close close" bindtap="__e"></text><view style="margin-bottom:40rpx;">是否提交审核?</view><view class="btns"><button data-event-opts="{{[['tap',[['closeModel',['$event']]]]]}}" class="cu-btn round bg-red shadow" bindtap="__e">取消</button><button data-event-opts="{{[['tap',[['sureSubmit',['$event']]]]]}}" class="cu-btn round bg-red shadow" bindtap="__e">确定</button></view></view></view></view>